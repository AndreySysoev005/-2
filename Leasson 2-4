/*4)Составьте программу, вычисляющую A*B, не пользуясь операцией
        Умножения.*/

package com.company;

public class Main {

    public static void main(String[] args) {
        int A=10;
        int B=10;
        int sum =0;
        for (int i=1; i<=B; i++) {
            sum = sum+A;
        }
        System.out.println(sum);
    }
}
