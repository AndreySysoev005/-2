/*
7)Напишите программу, определяющую сумму всех нечетных чисел от 1
До 99
 */
package com.company;

public class Main {

    public static void main(String[] args) {
        int sum=0;
	for (int i=1; i<=99; i++) {
	    if (i%2!=0) {
	        sum=sum+i;
        }
    } System.out.print(sum);
    }
}
